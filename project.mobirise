          "_customHTML": "<section class=\"ampTeam8\" group=\"Teams\" plugins=\"amp-main\">\n    <!--  -->\n    <mbr-parameters>\n        <header>Size:</header>\n        <input type=\"checkbox\" title=\"Full Width:\" name=\"fullWidth\">\n        <input type=\"checkbox\" title=\"Row/Column:\" name=\"flexDirection\" checked>\n        <input type=\"range\" inline title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <!--  -->\n        <header>Title:</header>\n        <input type=\"checkbox\" title=\"Title:\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle:\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Border:\" name=\"showBorderTitle\" condition=\"showTitle || showSubtitle\" checked>\n        <input type=\"color\" title=\"Color border:\" name=\"colorBorderTitle\" condition=\"showBorderTitle\" value=\"#7567f8\">\n        <select title=\"Align:\" name=\"alignTitle\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected>Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <!--  -->\n        <header>Cards:</header>\n        <input type=\"range\" inline title=\"Cards:\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"1\">\n        <select title=\"Align:\" name=\"cardAlign\" condition=\"showCardTitle || showCardSubtitle || showCardText || showSocial\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected>Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Name:\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle:\" name=\"showCardSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text:\" name=\"showCardText\" checked>\n        <input type=\"checkbox\" title=\"Transparent:\" name=\"transparentBg\" checked>\n        <input type=\"color\" title=\"Color BG:\" name=\"cardBg\" value=\"#efefef\" condition=\"!transparentBg\">\n        <!--  -->\n        <header>Image</header>\n        <input type=\"checkbox\" title=\"Rounded:\" name=\"roundedImage\" checked>\n        <input type=\"range\" inline title=\"Width:\" name=\"imageWidth\" min=\"10\" max=\"100\" step=\"1\" value=\"80\" condition=\"flexDirection\">\n        <input type=\"range\" inline title=\"Width:\" name=\"imageFlexWidth\" min=\"10\" max=\"100\" step=\"1\" value=\"100\" condition=\"!flexDirection\">\n        <!--  -->\n        <header>Icons</header>\n        <input type=\"checkbox\" title=\"Show Icon:\" name=\"showSocial\" checked>\n        <input type=\"range\" inline title=\"Icons:\" name=\"socialCount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showSocial\">\n        <input type=\"range\" inline title=\"Size Icon:\" name=\"iconsSize\" min=\"1\" max=\"4\" step=\"0.1\" value=\"1.3\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Color Icon:\" name=\"colorIcon\" value=\"#75757c\" condition=\"showSocial\">\n        <!--  -->\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image:\" value=\"../_images/background4.jpg\">\n            <input type=\"color\" title=\"Background Color:\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" checked condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#ffffff\" condition=\"bg.type!='color' && overlay\">\n        <input type=\"range\" inline title=\"Overlay Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"bg.type!='color' && overlay\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!--  -->\n    <!-- Container -->\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <!-- Title -->\n        <div class=\"title-block mbr-pb-5\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\">\n            <h3 class=\"mbr-section-title mbr-fonts-style mbr-pb-2 mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">Team</h3>\n            <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-pb-1 mbr-light\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\">How we work</h4>\n            <div class=\"line\" mbr-if=\"showBorderTitle && (showTitle || showSubtitle)\">\n                <div class=\"line-line-gray\">\n                    <div class=\"line-line-color\"></div>\n                </div>\n            </div>\n        </div>\n        <!-- Content -->\n        <div class=\"mbr-row mbr-jc-c mbr-pt-4\">\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-4 mbr-col-lg-4': cardsAmount < 4 &amp;&amp; flexDirection,\n            'mbr-col-md-6 mbr-col-lg-3':cardsAmount == 4 &amp;&amp; flexDirection,\n            'mbr-col-md-10 mbr-col-lg-6':!flexDirection,\n            'align-center':cardAlign==2,\n            'align-right':cardAlign==3,\n            'md-pb': (flexDirection &amp;&amp; cardsAmount > 2) || (!flexDirection &amp;&amp; cardsAmount > 1),\n            'lg-pb': !flexDirection &amp;&amp; cardsAmount > 2,\n            'last-child': cardsAmount == 1}\">\n            <div class=\"card-wrapper\" mbr-class=\"{'mbr-flex':!flexDirection}\">\n            <div class=\"card-img img-with-overlay\" mbr-class=\"{'mbr-m-auto':cardAlign==2 && flexDirection,\n            'mbr-ml-auto':cardAlign==3 && flexDirection,\n            'mbr-col-md-6':!flexDirection}\">\n            <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage}\" src=\"@PROJECT_PATH@/assets/images/shai-og-200x200.jpg\" layout=\"responsive\" width=\"268\" height=\"268\" alt=\"image\">\n            <div placeholder class=\"placeholder\"></div>\n            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/shai-og-200x200.jpg\" alt title>\n        </amp-img>\n    </div>\n    <div class=\"card-box\" mbr-class=\"{'mbr-pt-3 mbr-px-4 mbr-pb-3':!transparentBg,\n    'mbr-pt-3': transparentBg && flexDirection}\" mbr-if=\"showCardTitle || showCardSubtitle || showCardText || showSocial\">\n    <h5 class=\"main-color title-card mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".title-card\">Shai Perendik</h5>\n    <h6 mbr-text class=\"card-subtitle mbr-bold mbr-fonts-style mbr-pt-3\" mbr-theme-style=\"display-4\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-subtitle\">Founder &amp; President</h6>\n    <p class=\"card-text mbr-fonts-style mbr-pt-1\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">Shai has spent the last 20+ yrs in IT working on large scale projects. &nbsp;In his spare time he enjoys his wife and family of four, 3d printing, drones/fpv, and laser crafts.&nbsp;</p>\n    <div class=\"icons-list mbr-pt-4\" mbr-list mbr-list-grow=\"socialCount\" mbr-if=\"socialCount > 0 &amp;&amp; showSocial\" mbr-link-type=\"socials\">\n    <a href=\"https://www.facebook.com/shai.perednik\" target=\"_blank\">\n        <span class=\"iconfont-wrapper\">\n            <span mbr-icon class=\"amp-iconfont animation-normal fa-facebook fa\"></span>\n        </span>\n    </a>\n    <a href=\"http://twitter.com/shaiss\" target=\"_blank\">\n        <span class=\"iconfont-wrapper\">\n            <span mbr-icon class=\"amp-iconfont animation-normal fa-twitter fa\"></span>\n        </span>\n    </a>\n    <a href=\"http://instagram.com/shaiperednik\" target=\"_blank\" class=\"social\">\n        <span class=\"iconfont-wrapper\">\n            <span mbr-icon class=\"amp-iconfont fa-instagram fa\"></span>\n        </span>\n    </a>\n    \n</div>\n</div>\n</div>\n</div>\n<!--  -->\n<div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-4 mbr-col-lg-4': cardsAmount < 4 &amp;&amp; flexDirection,\n'mbr-col-md-6 mbr-col-lg-3':cardsAmount == 4 &amp;&amp; flexDirection,\n'mbr-col-md-10 mbr-col-lg-6':!flexDirection,\n'align-center':cardAlign==2,\n'align-right':cardAlign==3,\n'md-pb': (flexDirection &amp;&amp; cardsAmount > 2) || (!flexDirection &amp;&amp; cardsAmount > 2),\n'lg-pb': !flexDirection &amp;&amp; cardsAmount > 2,\n'last-child': cardsAmount == 2}\" mbr-if=\"cardsAmount>1\">\n<div class=\"card-wrapper\" mbr-class=\"{'mbr-flex':!flexDirection}\">\n<div class=\"card-img img-with-overlay\" mbr-class=\"{'mbr-m-auto':cardAlign==2 && flexDirection,\n'mbr-ml-auto':cardAlign==3 && flexDirection,\n'mbr-col-md-6':!flexDirection}\">\n<amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage}\" src=\"@PROJECT_PATH@/assets/images/face2.jpg\" layout=\"responsive\" width=\"100\" height=\"100\" alt=\"image\">\n<div placeholder class=\"placeholder\"></div>\n<img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/face2.jpg\" alt>\n</amp-img>\n</div>\n<div class=\"card-box\" mbr-class=\"{'mbr-pt-3 mbr-px-4 mbr-pb-3':!transparentBg,\n'mbr-pt-3': transparentBg && flexDirection}\" mbr-if=\"showCardTitle || showCardSubtitle || showCardText || showSocial\">\n<h5 class=\"main-color title-card mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".title-card\">asd</h5>\n<h6 mbr-text class=\"card-subtitle mbr-bold mbr-fonts-style mbr-pt-3\" mbr-theme-style=\"display-4\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-subtitle\">Founder</h6>\n<p class=\"card-text mbr-fonts-style mbr-pt-1\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">Lorem ipsum dolor sit amet, consectetur adi